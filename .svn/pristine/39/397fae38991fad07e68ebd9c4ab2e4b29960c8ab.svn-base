.radios {  float: left;  width: 16px;height: 16px;  background: url(../images/radioNot.png) no-repeat;  margin-top: 6px;  margin-right: 8px;}
.radio-pitch .radios {  background: url(../images/radio.png) no-repeat;}

.checkbox {  float: left;  width: 16px;height: 16px; background: url(../images/checkboxNot.png) no-repeat;  margin-top: 6px;  margin-right: 8px;   }
