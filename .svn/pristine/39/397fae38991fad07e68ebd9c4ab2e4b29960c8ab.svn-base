/*等宽选项*/
.problem .uniformWidth .qula li{width: 200px;margin-right: 10px;  padding-top: 5px;  cursor: pointer;  display: inline-block;  line-height: 28px; color: #989898;  font-size: 12px;  }
.problem .noUniformWidth .qula li{margin-right: 10px; padding-top: 5px;  cursor: pointer;  display: block;  line-height: 28px;  color: #989898;  font-size: 12px;  }

